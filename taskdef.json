{
  "family": "task-multiplicacion3",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::708110116686:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::708110116686:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "multiplicacion3",
      "image": "<IMAGE_NAME>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/task-multiplicacion3",
          "awslogs-create-group": "true",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      }
    }
  ]
}
